{
  "name": "dnd-fichas",
  "version": "1.0.0",
  "description": "D&D Character Sheets App",
  "main": "main.js",
  "scripts": {
    "dev": "npm run build:react && electron . --dev",
    "start": "npm run build:react && electron .",
    "build:react": "npm run build --prefix client",
    "build:web": "npm run build:react",
    "build:android": "npm run build:web && npx cap copy android && npx cap open android"
  },
  "keywords": [
    "dnd",
    "electron",
    "character-sheets"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "electron": "^37.2.3"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2"
  }
}
